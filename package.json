{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path ./dist",
    "build:ci": "ng build --output-path ./dist --localize",
    "build:static": "ng build --output-path ./dist --localize --ssr=false --output-mode=static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:website": "node dist/website/server/server.mjs",
    "extract-i18n": "ng extract-i18n --out-file src/locales/en_devel.xlf --format xlif"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@angular/ssr": "^20.1.3",
    "@feel/form": "^0.1.1",
    "@feel/notification": "^0.1.1",
    "@feel/style": "^0.1.1",
    "@fontsource/dm-sans": "^5.2.6",
    "apexcharts": "^5.3.1",
    "express": "^5.1.0",
    "ngx-progressbar": "^14.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@angular/localize": "20.1.3",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^22.16.5",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
